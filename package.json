{
	"private": true,
	"name": "kotlin-js-webpack-example",
	"version": "0.0.1",
	"scripts": {
		"start": "NODE_ENV=development webpack-dev-server --mode development",
		"build": "NODE_ENV=production webpack --mode production --progress --profile --bail --display-error-details",
		"test": "jest",
		"clean": "rimraf dist kotlin_build",
		"rm": "rimraf dist kotlin_build node_modules",
		"lint": "eslint src",
		"source-map": "NODE_ENV=production webpack --mode production --devtool='source-map'"
	},
	"devDependencies": {
		"@jetbrains/kotlin-webpack-plugin": "^1.2.2",
		"css-loader": "^0.28.10",
		"mini-css-extract-plugin": "^0.2.0",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.0.6",
		"source-map-loader": "^0.2.3",
		"style-loader": "^0.20.3",
		"webpack": "^4.1.1",
		"webpack-cli": "^2.0.11",
		"webpack-dev-server": "^3.1.1"
	},
	"dependencies": {
		"kotlin": "^1.2.30"
	}
}
